
body,
html {
	height: 100%;
	padding: 0;
	margin: 0;
}

.infoBubble {
	padding: 0;
	margin: 0;
	border: 0;
	font-size: 1.2em;
}

.places {
	height: 200px;
	width: 300px;
	float: left;
	z-index: 5;
	top: 56px;
	position: absolute;
	overflow: auto;
	margin-left: 2%;
	font-size: 1.2em;
}

.map-canvas {
	height: 100%;
	width: 100%;	
	overflow: auto;
	padding: 0;
}

h3 {
	position: absolute;
	z-index: 5;
	top: 0;
	margin-left: 3%;
	background-color: #dbf0f9;
	color: Black;

}

.placesBtn {
	float: left;
	z-index: 5;
	top: 80%;
}

.bg { 
 	width: 275px; 
 	height: 160px;	
} 

input,
textarea {
	background-color: black;
	color: MintCream;
}

ul {
	list-style-type: none;
	padding: 10px;
	margin: 10px;
	background-color: Navy;
	color: MintCream;
	line-height: 28px;
}

li:hover {
	background: lime;
	color: black;
}

.selected {
	background: MediumBlue;

}
